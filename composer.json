{
    "name": "gdatacyberdefenseag/gdata-antivirus",
    "version": "1.2.3",
    "autoload": {
        "psr-4": {
            "Gdatacyberdefenseag\\GdataAntivirus\\": "."
        }
    },
    "authors": [
        {
            "name": "Simonis, Matthias",
            "email": "Matthias.Simonis@gdata.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "gdata/vaas": "11.0.1",
        "illuminate/container": "^11.30 || ^10 || ^12"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "cs:check": "vendor/bin/phpcs",
        "cs:fix": "vendor/bin/phpcbf"
    },
    "require-dev": {
        "johnpbloch/wordpress": "6.8.3",
        "phpunit/phpunit": "^12.0 || ^11.0 || ^10.0",
        "wp-coding-standards/wpcs": "^3.1",
        "symfony/finder": "^7.1 || ^6.0"
    }
}